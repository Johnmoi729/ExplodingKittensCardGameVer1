{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "MongoDbSettings": {
      "ConnectionString": "#{CosmosDbConnectionString}#",
      "DatabaseName": "ExplodingKittens"
    },
    "JwtSettings": {
